    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "prettier": "^3.0.3",
    "sass": "^1.80.6",
    "stylelint": "^16.10.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "typescript": "^5.6.3",
    "vite": "^5.4.8",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^2.1.2"
  }
